{
    "background": {
        "scripts": [
            "/lib/jquery-2.1.4.min.js"
        ]
    },
    "name": "Website Script Inject",
    "version": "1.0.1",
    "manifest_version": 2,
    "minimum_chrome_version": "41",
    "description": "网站Script植入工具",
    "homepage_url": "http://note.yinziqiang.com/",
    "browser_action": {
        "default_icon": {
            "19": "img/icon_19x19.png",
            "38": "img/icon_38x38.png"
        },
        "default_popup": "popup.html",
        "default_title": "YZQ_Extension"
    },
    "icons": {
        "128": "img/icon_128x128.png",
        "16": "img/icon_16x16.png",
        "48": "img/icon_48x48.png"
    },
    "web_accessible_resources": [
        "img/icon_38x38.png",
        "js/popup.js"
    ],
    "permissions": [
        "tabs",
        "\u003Call_urls>",
        "cookies",
        "contextMenus",
        "unlimitedStorage",
        "notifications",
        "storage",
        "clipboardWrite"
    ]
}